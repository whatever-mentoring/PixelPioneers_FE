@font-face {
  font-family: 'Pretendard-Regular';
  src: url('../resource/font/Pretendard-Regular.woff') format('woff');
  font-weight: 400;
  font-style: normal;
}

.AppLogin {
  background-color: #1C54A9;
  text-align: center;
  color : white;
  height: 100vh;
  width: 100%;
  font-family: 'Pretendard-Regular';
}
.tag {
  color: white;
  
}
.tag1 {
  text-align: left;
  position: absolute;
  left: 5vh;
  top: 10vh;
}
.tag2 {
  font-size: 40px;
  text-align: left;
  position: absolute;
  left: 5vh;
  top: 10vh;
}

.vector1 {
  background-image: url("../resource/Vector 2.png");
  position: relative;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  width: 100%;
  height: 60vh;

  
}
a,a:visited {
  text-decoration: none;

}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container > .naver_button,.kakao_button,.apple_button{
  display: flex;
  position: relative;
  width: 30vh;
  line-height: 5vh;
  text-align: center;
  top: -5vh;
  justify-content: center;
  border-radius: 18px;
  font-size: 15px;
}

/*
<div class="naverlogo"></div>
<div class="kakaologo"></div>
<div class="applelogo"></div>
*/

.naver_button{
  color: white;
  background-color: #3FDE25;
  margin-bottom: 4vh;
}


.naver_button:hover{
  transform: scale(1.1,1.1);
  box-shadow: 0px 5px 5px -2px rgba(0, 0, 0, 0.25);
}
.kakao_button{
  color: black;
  background-color: #EFF23E;
  margin-bottom: 4vh;
  
}
.kakao_button:hover{
  transform: scale(1.1,1.1);
  box-shadow: 0px 5px 5px -2px rgba(0, 0, 0, 0.25);
}
.apple_button{
  color: black;
  background-color: #FFFFFF;
}
.apple_button:hover{
  transform: scale(1.1,1.1);
  box-shadow: 0px 5px 5px -2px rgba(0, 0, 0, 0.25);
}
.naverlogo{
  background-image: url("../resource/naver-fill\ 1.png");
  background-repeat: no-repeat;
  position:relative;
  height: 4vh;
  width: 4vh;
  top: 1.5vh;
  left: -3vh;
}
.kakaologo{
  background-image: url("../resource/kakaotalk 1.png");
  background-repeat: no-repeat;
  position: relative;
  height: 4vh;
  width: 4vh;
  left: -3.4vh;
  top: 1vh;
  
}
.applelogo{
  background-image: url("../resource/Vector.png");
  background-repeat: no-repeat;
  position: relative;
  height: 4vh;
  width: 4vh;
  left: -3.5vh;
  top: 1.5vh;
}